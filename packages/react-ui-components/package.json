{
  "name": "react-ui-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyassets:all": "yarn run copyassets:md-font",
    "copyassets:md-font": "copyfiles -u 5 \"../../node_modules/material-design-icons/iconfont/*\" \"storybook-static/fonts/material-icons\"",
    "storybook": "yarn run copyassets:all && storybook dev -p 9011",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "classnames": "^2.3.2",
    "material-components-web": "^5.1.0",
    "material-design-icons": "^3.0.1",
    "rc-tooltip": "^6.0.1",
    "react-router-dom": "^6.13.0",
    "storybook-dark-mode": "^3.0.0"
  }
}
