{
  "name": "@map-colonies/react-core",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/@map-colonies/react-core.umd.js",
  "module": "./dist/@map-colonies/react-core.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "transpileTS": "tsc --project ./tsconfig-build.json",
    "build": "yarn run clean && yarn printTSVersion && yarn transpileTS && vite build",
    "printTSVersion": "echo TS $(tsc -v)",
    "postbuild": "copyfiles -u 2 -e \"**/*.ts\" -e \"**/*.tsx\" \"src/components/**/*\" dist",
    "prepack": "yarn build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyassets:all": "yarn run copyassets:md-font",
    "copyassets:md-font": "copyfiles -u 5 \"../../node_modules/material-design-icons/iconfont/*\" \"./public/fonts/material-icons\"",
    "storybook": "yarn run copyassets:all && storybook dev -p 9011",
    "build-storybook": "storybook build && yarn run copyassets:md-font"
  },
  "dependencies": {
    "@rmwc/avatar": "^8.0.8",
    "@rmwc/button": "^8.0.8",
    "@rmwc/checkbox": "^8.0.8",
    "@rmwc/circular-progress": "^8.0.8",
    "@rmwc/dialog": "^8.0.8",
    "@rmwc/drawer": "^8.0.8",
    "@rmwc/fab": "^8.0.8",
    "@rmwc/icon": "^8.0.8",
    "@rmwc/icon-button": "^8.0.8",
    "@rmwc/linear-progress": "^8.0.8",
    "@rmwc/menu": "^8.0.8",
    "@rmwc/select": "^8.0.8",
    "@rmwc/snackbar": "^8.0.8",
    "@rmwc/switch": "^8.0.8",
    "@rmwc/tabs": "^8.0.8",
    "@rmwc/textfield": "^8.0.8",
    "@rmwc/theme": "^8.0.8",
    "@rmwc/tooltip": "^8.0.8",
    "@rmwc/typography": "^8.0.8",
    "material-design-icons": "^3.0.1",
    "storybook-dark-mode": "^3.0.0",
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@rmwc/types": "^8.0.8",
    "@types/node": "^20.3.1",
    "rimraf": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "@rmwc/menu": "^8.0.8",
    "tslib": "^2.5.3"
  },
  "workspaces": {
    "nohoist": [
      "tslib",
      "@rmwc/*",
      "react",
      "react-dom",
      "@types/react",
      "@types/react-dom"
    ]
  }
}
