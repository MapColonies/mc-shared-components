{
  "name": "@map-colonies/react-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "yarn run clean && tsc --project tsbuildconfig.json && vite build",
    "postbuild": "copyfiles -u 2 -e \"**/*.ts\" -e \"**/*.tsx\" \"src/components/**/*\" dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyassets:all": "yarn run copyassets:md-font",
    "copyassets:md-font": "copyfiles -u 5 \"../../node_modules/material-design-icons/iconfont/*\" \"storybook-static/fonts/material-icons\"",
    "storybook": "yarn run copyassets:all && storybook dev -p 9011",
    "build-storybook": "storybook build && yarn run copyassets:md-font"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "classnames": "^2.3.2",
    "material-components-web": "^8.0.0",
    "material-design-icons": "^3.0.1",
    "rc-tooltip": "^6.0.1",
    "react-ga": "^3.3.1",
    "react-live": "^4.1.3",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.13.0",
    "storybook-dark-mode": "^3.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.13",
    "rimraf": "^5.0.1",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
