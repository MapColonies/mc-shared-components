{
  "name": "@map-colonies/react-components",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/@map-colonies/react-components.umd.js",
  "module": "./dist/@map-colonies/react-components.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "transpileTS": "tsc --project ./tsconfig-build.json",
    "build": "yarn run clean && yarn printTSVersion && yarn transpileTS && vite build",
    "printTSVersion": "echo TS $(tsc -v)",
    "postbuild": "copyfiles -u 2 -e \"**/*.ts\" -e \"**/*.tsx\" \"src/components/**/*\" dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copyassets:all": "yarn run copyassets:md-font && yarn run copyassets:cesium",
    "copyassets:cesium": "copyfiles -u 5 \"../../node_modules/cesium/Build/Cesium/**/*\" \"storybook-static\"",
    "copyassets:md-font": "copyfiles -u 5 \"../../node_modules/material-design-icons/iconfont/*\" \"storybook-static/fonts/material-icons\"",
    "storybook": "yarn run copyassets:all && storybook dev -p 9010",
    "build-storybook": "storybook build && yarn run copyassets:md-font"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@here/quantized-mesh-decoder": "^1.2.8",
    "@map-colonies/react-core": "0.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/helpers": "^6.1.4",
    "@turf/rewind": "^5.1.5",
    "@types/geojson": "^7946.0.10",
    "@types/ol": "6.3.1",
    "cesium": "1.103.0",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "copy-webpack-plugin": "^6.3.1",
    "copyfiles": "2.3.0",
    "cross-env": "7.0.2",
    "date-fns": "^2.14.0",
    "geojson": "^0.5.0",
    "get-input-selection": "^1.1.4",
    "lodash": "^4.17.20",
    "ol": "^6.4.3",
    "resium": "^1.16.1",
    "rimraf": "3.0.2",
    "textarea-caret": "^3.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.165",
    "@types/react-test-renderer": "^16.9.2",
    "@types/textarea-caret": "^3.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest-enzyme": "^7.1.2"
  }
}
